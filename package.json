{
	"name": "git-flow",
	"version": "0.1.0",
	"description": "**adding start, publish and finish feature**",
	"main": "app.js",
	"dependencies": {
		"commitizen": "^2.8.2",
		"conventional-changelog": "^1.1.0",
		"conventional-recommended-bump": "^0.2.1"
	},
	"devDependencies": {
		"cz-conventional-changelog": "^1.1.6",
		"git-release-note": "^0.1.1",
		"semantic-release-gitflow": "github:gitawego/semantic-release-gitflow"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"next-version": "conventional-recommended-bump -p angular -v",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
		"gh-release-notes": "conventional-github-releaser -p angular -r 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tlimpanont/git-flow.git"
	},
	"author": "Theuy Limpanont <info@theuy.nl> (http://theuy.nl/)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/tlimpanont/git-flow/issues"
	},
	"homepage": "https://github.com/tlimpanont/git-flow#readme",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
